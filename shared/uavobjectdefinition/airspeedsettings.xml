<xml>
    <object name="AirspeedSettings" singleinstance="true" settings="true">
        <description>Settings for the @ref BaroAirspeed module used on CopterControl or Revolution</description>
        <field name="GPSSamplePeriod" units="ms" type="uint8" elements="1" defaultvalue="100"/>
        <field name="ZeroPoint" type="uint16" elements="1" defaultvalue="0"/>
        <field name="Scale" type="float" elements="1" defaultvalue="1.0"/>
        <field name="AirspeedSensorType" type="enum" elements="1" options="EagleTreeAirspeedV3,DIYDronesMPXV5004,DIYDronesMPXV7002,GPSOnly" defaultvalue="GPSOnly"/>
        <field name="AnalogPin" type="enum" elements="1" options="ADC0,ADC1,ADC2,ADC3,ADC4,ADC5,ADC6,ADC7,ADC8,NONE" defaultvalue="NONE"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="true" updatemode="onchange" period="0"/>
        <telemetryflight acked="true" updatemode="onchange" period="0"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
