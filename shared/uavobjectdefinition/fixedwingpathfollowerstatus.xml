<xml>
    <object name="FixedWingPathFollowerStatus" singleinstance="true" settings="false">
        <description>Object Storing Debugging Information on PID internals</description>
        <field name="Error" type="float" elementnames="Bearing,Speed,Accel,Power" />
        <field name="ErrorInt" type="float" elementnames="Bearing,Speed,Accel,Power" />
        <field name="Command" type="float" elementnames="Bearing,Speed,Accel,Power" />
        <field name="Errors" type="uint8" elementnames="Wind,Stallspeed,Lowspeed,Highspeed,Overspeed,Lowpower,Highpower,Pitchcontrol" />
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="throttled" period="50000"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
