<xml>
    <object name="GPSPosition" singleinstance="true" settings="false">
        <description>Raw GPS data from @ref GPSModule. </description>
        <field name="Status" type="enum" elements="1" options="NoGPS,NoFix,Fix2D,Fix3D,Diff3D"/>
        <field name="Latitude" units="degrees x 10^-7" type="int32" elements="1"/>
        <field name="Longitude" units="degrees x 10^-7" type="int32" elements="1"/>
        <field name="Altitude" units="m" type="float" elements="1"/>
        <field name="GeoidSeparation" units="m" type="float" elements="1"/>
        <field name="Heading" units="degrees" type="float" elements="1"/>
        <field name="Groundspeed" units="m/s" type="float" elements="1"/>
        <field name="Satellites" type="uint8" elements="1"/>
        <field name="Accuracy" units="m" type="float" elements="1"/>
        <field name="PDOP" type="float" elements="1"/>
        <field name="HDOP" type="float" elements="1"/>
        <field name="VDOP" type="float" elements="1"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="throttled" period="2000"/>
        <logging updatemode="periodic" period="1000"/>
	</object>
</xml>
