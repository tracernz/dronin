				<option>OpenLog</option>
				<option>PicoC</option>
				<option>S.Bus Non Inverted</option>
				<option>Storm32Bgc</option>
				<option>Telemetry</option>
			</options>
		</field>

		<field name="ADCInputs" type="enum" elements="1"  defaultvalue="Disabled">
			<options>
				<option>Disabled</option>
				<option>Enabled</option>
			</options>
		</field>
		
		<field name="USB_HIDPort" type="enum" elements="1" options="USBTelemetry,Disabled" defaultvalue="USBTelemetry"/>
		<field name="USB_VCPPort" type="enum" elements="1" options="USBTelemetry,ComBridge,DebugConsole,PicoC,Disabled" defaultvalue="Disabled"/>

		<field name="DSMxMode" type="enum" elements="1" parent="HwShared.DSMxMode" defaultvalue="Autodetect"/>

		<field name="GyroRange" units="deg/s" type="enum" elements="1" options="250,500,1000,2000" defaultvalue="1000"/>
		<field name="AccelRange" units="G" type="enum" elements="1" options="2,4,8,16" defaultvalue="8"/>
		<field name="MPU6000Rate" units="Hz" type="enum" elements="1" options="200,333,500,666,1000,2000,4000,8000" defaultvalue="500"/>
		<field name="MPU6000DLPF" units="Hz" type="enum" elements="1" options="256,188,98,42,20,10,5" defaultvalue="188"/>
	
		<field name="Magnetometer" type="enum" elements="1" options="Disabled,Internal,External" defaultvalue="Internal"/>
		<field name="ExtMagOrientation" type="enum" elements="1" options="Top0degCW,Top90degCW,Top180degCW,Top270degCW,Bottom0degCW,Bottom90degCW,Bottom180degCW,Bottom270degCW" defaultvalue="Top0degCW" />
	
		<access gcs="readwrite" flight="readwrite"/>
		<telemetrygcs acked="true" updatemode="onchange" period="0"/>
		<telemetryflight acked="true" updatemode="onchange" period="0"/>
		<logging updatemode="manual" period="0"/>
	</object>
</xml>
