<xml>
    <object name="MixerStatus" singleinstance="true" settings="false">
        <description>Status for the matrix mixer showing the output of each mixer after all scaling</description>
        <field name="Mixer1" type="float" elements="1"/>
        <field name="Mixer2" type="float" elements="1"/>
        <field name="Mixer3" type="float" elements="1"/>
        <field name="Mixer4" type="float" elements="1"/>
        <field name="Mixer5" type="float" elements="1"/>
        <field name="Mixer6" type="float" elements="1"/>
        <field name="Mixer7" type="float" elements="1"/>
        <field name="Mixer8" type="float" elements="1"/>
        <field name="Mixer9" type="float" elements="1"/>
        <field name="Mixer10" type="float" elements="1"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
