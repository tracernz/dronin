<xml>
	<object name="OnScreenDisplaySettings" singleinstance="true" settings="true">
		<description>On Sreen Display Settings</description>
		<field name="OSDEnabled" type="enum" elements="1" options="Disabled,Enabled" defaultvalue="Enabled"/>
		<field name="Units" type="enum" elements="1" options="Metric,Imperial" defaultvalue="Metric"/>
		<field name="PALWhite" type="uint8"  elements="1" defaultvalue="110"/>
		<field name="PALBlack" type="uint8"  elements="1" defaultvalue="35"/>
		<field name="NTSCWhite" type="uint8"  elements="1" defaultvalue="110"/>
		<field name="NTSCBlack" type="uint8"  elements="1" defaultvalue="20"/>
		<field name="XOffset" units="px"  type="int8"  elements="1" defaultvalue="0"/>
		<field name="YOffset" units="px"  type="int8"  elements="1" defaultvalue="0"/>
		<field name="NumPages" type="int8"  elements="1" defaultvalue="2"/>
		<field name="PageSwitch" type="enum" elements="1" options="Accessory0,Accessory1,Accessory2" defaultvalue="Accessory0"/>
		<field name="PageConfig" type="enum" elements="6" options="Custom1,Custom2,Custom3,Custom4,Menu,Statistics,Off"
		       defaultvalue="Custom1,Custom1,Custom1,Custom1,Custom1,Custom1"/>
		<field name="DisableMenuWhenArmed" type="enum" elements="1" options="Disabled,Enabled" defaultvalue="Enabled"/>
		<field name="RssiWarnThreshold" type="uint8" elements="1" defaultvalue="50"/>
		<field name="CustomText" type="uint8" elements="10" defaultvalue="66,114,97,105,110,70,80,86,0,0"/>
		<field name="StatsDisplayDuration" units="s"  type="enum" elements="1" options="Off,10,20,30" defaultvalue="20"/>
	<access gcs="readwrite" flight="readwrite"/>
	<telemetrygcs acked="true" updatemode="onchange" period="0"/>
	<telemetryflight acked="true" updatemode="onchange" period="0"/>
	<logging updatemode="manual" period="0"/>
	</object>
</xml>
