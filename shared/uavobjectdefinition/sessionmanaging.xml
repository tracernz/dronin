<xml>
  <object name="SessionManaging" singleinstance="true" settings="false">
    <description>Provides session managing to uavtalk</description>
      <field name="SessionID" type="uint16" elements="1"/>
      <field name="ObjectID" type="uint32" elements="1"/>
      <field name="ObjectInstances" type="uint8" elements="1"/>
      <field name="NumberOfObjects" type="uint8" elements="1"/>
      <field name="ObjectOfInterestIndex" type="uint8" elements="1"/>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="manual" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
  </object>
</xml>
