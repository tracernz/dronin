<xml>
    <object name="UBloxInfo" singleinstance="true" settings="false">
        <description>UBlox specific information</description>
        <field name="swVersion" type="uint32" elements="1"/>
        <field name="hwVersion" type="uint16" elements="1"/>
        <field name="ParseErrors" type="uint32" elements="1"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="periodic" period="10000"/>
        <logging updatemode="periodic" period="1000"/>
	</object>
</xml>
